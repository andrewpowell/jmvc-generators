steal('can','./<%= underscore %>.mustache', function(can, template){
    /**
     * @class <%= module %>
	 * @alias <%= Alias %>   
     */
    return can.Control(
	/** @Static */
	{
		defaults : {}
	},
	/** @Prototype */
	{
		model: null,
		viewData: null,
	
		init : function(){
			this.initHTML();
		},
		
		initHTML : function(){
			var renderedView = can.view(template);
			this.element.html(renderedView);
		}
	});
});